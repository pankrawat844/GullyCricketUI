    <TextView
        android:id="@+id/textView28"
        android:layout_width="@dimen/dp_20"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:fontFamily="@font/googlesans_bold"
        android:gravity="end"
        android:text="B"
